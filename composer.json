{
    "name": "desmg/registry-sync",
    "type": "project",
    "require": {
        "php": "^8.4",
        "illuminate/http": "^12.17",
        "laravel-zero/framework": "^12.0.2"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "sync"
    ]
}
